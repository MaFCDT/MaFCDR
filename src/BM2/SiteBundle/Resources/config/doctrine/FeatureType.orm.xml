<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping>
	<entity name="BM2\SiteBundle\Entity\FeatureType" table="types.featuretype">
		<id name="id" type="integer">
			<generator strategy="IDENTITY"/>
		</id>
		<field name="name" type="string"/>
		<field name="hidden" type="boolean"/>
		<field name="icon" type="string" nullable="true"/>
		<field name="icon_under_construction" type="string" nullable="true"/>
		<field name="build_hours" type="integer"/>
	</entity>
</doctrine-mapping>
